<?xml version="1.0" encoding="utf-8"?>


<robot name="echo_v1" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Include materials xacro -->
  <xacro:include filename="$(find echo_sim_v1)/urdf/materials.xacro"/>

  <!-- Base Footprint -->
  <link name="base_footprint"/>

  <joint name="base_footprint_to_base_link" type="fixed">
    <origin xyz="0 0 0.02" rpy="-1.5708 0 0"/>
    <parent link="base_footprint"/>
    <child link="base_link"/>
  </joint>

  <!-- Base Link -->
  <link name="base_link">
    <inertial>
      <origin xyz="0.00088588 5.7031E-05 0.02478" rpy="0 0 0"/>
      <mass value="0.99833"/>
      <inertia ixx="0.0010305" ixy="-1.7168E-06" ixz="1.5313E-05"
               iyy="0.0035021" iyz="1.7459E-07" izz="0.0041088"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/base_link.STL"/>
      </geometry>
      <material name="red"/>
    </visual>
    <!-- <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
       <box size="0.3 0.3 0.1"/>
      </geometry>
      <material name="Red"/>
    </visual> -->

    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/base_link.STL"/>
      </geometry>
    </collision>
  </link>

  <!-- Left Wheel -->
  <link name="left_wheel_link">
    <inertial>
      <origin xyz="-1.3878E-17 0.0075 0" rpy="0 0 0"/>
      <mass value="0.073711"/>
      <inertia ixx="3.0207E-05" ixy="-3.1866E-38" ixz="1.075E-37"
               iyy="5.7735E-05" iyz="1.0613E-38" izz="3.0207E-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/left_wheel_link.STL"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/left_wheel_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link_to_left_wheel" type="continuous">
    <origin xyz="-0.07 -0.02 0.05" rpy="-1.5708 0 -1.5708"/>
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <axis xyz="0 -1 0"/>
  </joint>

  <!-- Right Wheel -->
  <link name="right_wheel_link">
    <inertial>
      <origin xyz="-1.3878E-17 -0.0075 0" rpy="0 0 0"/>
      <mass value="0.073711"/>
      <inertia ixx="3.0207E-05" ixy="-3.1866E-38" ixz="1.075E-37"
               iyy="5.7735E-05" iyz="1.0613E-38" izz="3.0207E-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/right_wheel_link.STL"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/right_wheel_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link_to_right_wheel" type="continuous">
    <origin xyz="-0.07 -0.02 -0.05" rpy="1.5708 0 0"/>
    <parent link="base_link"/>
    <child link="right_wheel_link"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- Caster Wheel -->
  <link name="caster_wheel_link">
    <inertial>
      <origin xyz="1.3878E-17 -5.5511E-17 0.0045" rpy="0 0 0"/>
      <mass value="0.0036191"/>
      <inertia ixx="1.3517E-07" ixy="-1.3235E-23" ixz="-8.7993E-24"
               iyy="1.3517E-07" iyz="-5.2645E-22" izz="2.0846E-07"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/caster_wheel_link.STL"/>
      </geometry>
      <material name="silver"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/caster_wheel_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link_to_caster_wheel" type="fixed">
    <origin xyz="0.07 0.008 0" rpy="1.5708 -1.2948 3.1416"/>
    <parent link="base_link"/>
    <child link="caster_wheel_link"/>
    <axis xyz="0 0 0"/>
  </joint>

  <!-- Lidar -->
  <link name="lidar_link">
    <inertial>
      <origin xyz="-2.6195E-17 -1.1719E-17 0.0097699" rpy="0 0 0"/>
      <mass value="0.024496"/>
      <inertia ixx="3.1849E-06" ixy="3.1764E-22" ixz="-1.8127E-23"
               iyy="3.1849E-06" iyz="-3.5234E-23" izz="4.7979E-06"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/lidar_link.STL"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/lidar_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link_to_lidar" type="fixed">
    <origin xyz="0 -0.05 0" rpy="1.5708 0.30388 0"/>
    <parent link="base_link"/>
    <child link="lidar_link"/>
    <axis xyz="0 0 0"/>
  </joint>

  <!-- Camera  -->
  <link name="camera_link">
    <inertial>
      <origin xyz="0.0024702 4.5609E-18 -6.9389E-18" rpy="0 0 0"/>
      <mass value="0.00094737"/>
      <inertia ixx="5.0737E-08" ixy="5.1013E-24" ixz="5.311E-25"
               iyy="6.9275E-09" iyz="7.8632E-15" izz="4.7598E-08"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/camera_link.STL"/>
      </geometry>
      <material name="dark_grey"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://echo_sim_v1/meshes/camera_link.STL"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_link_to_camera" type="fixed">
    <origin xyz="0.1 -0.049 0" rpy="1.5708 0 0"/>
    <parent link="base_link"/>
    <child link="camera_link"/>
    <axis xyz="0 0 0"/>
  </joint>

</robot>
